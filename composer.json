{
    "name"         : "jbzoo/cli",
    "type"         : "library",
    "description"  : "__DESCRIPTION__",
    "license"      : "MIT",

    "authors"      : [
        {
            "name"  : "Denis Smetannikov",
            "email" : "admin@jbzoo.com",
            "role"  : "lead"
        }
    ],

    "require"      : {
        "php"                                 : ">=7.2",

        "jbzoo/utils"                         : "^4.4.0",
        "symfony/process"                     : "^4.4|^5.0",
        "symfony/console"                     : "^4.4|^5.0",
        "symfony/lock"                        : "^4.4|^5.0",
        "bluepsyduck/symfony-process-manager" : "^1.3.2"
    },

    "require-dev"  : {
        "jbzoo/toolbox-dev" : "^3.0.0"
    },

    "autoload"     : {
        "psr-4" : {
            "JBZoo\\Cli\\" : "src"
        }
    },

    "autoload-dev" : {
        "classmap" : ["tests"]
    },

    "config"       : {
        "optimize-autoloader" : true
    },

    "extra"        : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    }
}
